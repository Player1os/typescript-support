{
	"compilerOptions": {
		"experimentalDecorators": true,
		"module": "commonjs",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitUseStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"inlineSourceMap": true,
		"strictNullChecks": true,
		"target": "es2017"
	}
}
